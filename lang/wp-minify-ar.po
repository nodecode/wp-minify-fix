#, php-format
msgid "%s options has been updated."
msgstr "تم تحديث إعدادات %s."

#: common.php:94
#, php-format
msgid "%s options has been upgraded."
msgstr "تم تحديث إعدادات %s."

#: common.php:100
#, php-format
msgid "%s options has been reset."
msgstr "تم تصفير إعدادات %s."

#: common.php:106
#, php-format
msgid "%s cache has been cleared."
msgstr "تم نظيف النسخة المخزنة لـ %s."

#: common.php:112
#, php-format
msgid "%s options imported."
msgstr "تم استيراد إعدادات %s"

